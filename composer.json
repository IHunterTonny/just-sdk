{
    "name": "just/api",
    "description": "SDK library for public API",
    "type": "library",
    "authors": [
        {
            "name": "IHunterTonny",
            "email": "i.huntertonny@gmail.com"
        }
    ],
    "autoload": {
        "psr-4": {
            "Just\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Just\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "require": {
        "php": ">=7.4",
        "php-http/client-common": "^2.5",
        "psr/http-client": "^1.0",
        "psr/http-factory": "^1.0",
        "symfony/options-resolver": "^5.2"
    },
    "require-dev": {
        "laminas/laminas-diactoros": "^2.9",
        "php-http/curl-client": "^2.2",
        "php-http/mock-client": "^1.5",
        "phpunit/phpunit": "^9.5"
    }
}
